<window id="1234" type="dialog">
    <backgroundcolor>backgroundcolor</backgroundcolor>
    <allowoverlay>no</allowoverlay>
    <visible>true</visible>
    <coordinates>
        <system>1</system>
        <posX>200</posX>
        <posY>100</posY>
    </coordinates>
    <controls>
        <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>1530</width>
            <height>720</height>
            <texture border="20,0,0,0" flipx="true">background2.png</texture>
        </control>

        <control type="image">
            <posx>200</posx>
            <posy>20</posy>
            <width>470</width>
            <height>540</height>
            <texture border="50">panel.png</texture>
            <colordiffuse>$VAR[DialogColorVar]</colordiffuse>
        </control>

        <!--title separator -->
        <control type="image">
            <posx>220</posx>
            <posy>60</posy>
            <width>440</width>
            <height>30</height>
            <texture>settings/settings_separator.png</texture>
        </control>
        <!-- end title separator -->

        <!--up-down buttons -->
        <control type="group" id="114">
            <visible>true</visible>
            <posx>-350</posx>
            <posy>-420</posy>
            <height>100</height>
            <width>200</width>
            <control type="button" id="112">
                <posx>30</posx>
                <posy>-40</posy>
                <width>75</width>
                <height>122</height>
                <texturefocus>settings/spin-down-focus.png</texturefocus>
                <texturenofocus>settings/spin-down.png</texturenofocus>
                <include>Default_SettingButton</include>
            </control>
            <control type="button" id="113">
                <posx>105</posx>
                <posy>-40</posy>
                <width>75</width>
                <height>122</height>
                <texturefocus>settings/spin-up-focus.png</texturefocus>
                <texturenofocus>settings/spin-up.png</texturenofocus>
                <include>Default_SettingButton</include>
            </control>
        </control>
        <!--end up-down buttons -->

        <!--switch buttons -->
        <control type="group" id="118">
            <visible>true</visible>
            <posx>-350</posx>
            <posy>-120</posy>
            <height>50</height>
            <width>150</width>
            <control type="label" id="115">
                <posx>40</posx>
                <posy>0</posy>
                <width>50</width>
                <height>50</height>
                <visible>true</visible>
                <align>center</align>
                <aligny>center</aligny>
                <label>AN</label>
                <font>Font_Reg29</font>
            </control>
            <control type="radiobutton" id="116">
                <posx>190</posx>
                <posy>0</posy>
                <width>30</width>
                <height>50</height>
                <visible>true</visible>
                <textureradioon>settings/settings_radiobuttononfocus.png</textureradioon>
                <textureradiooff>settings/settings_radiobuttonon.png</textureradiooff>
                <texturefocus border="5">-</texturefocus>
                <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                <texturenofocus>-</texturenofocus>
                <focusedcolor>-</focusedcolor>
                <label>-</label>
                <align>center</align>
                <aligny>center</aligny>
            </control>
            <control type="label" id="117">
                <posx>200</posx>
                <posy>0</posy>
                <width>50</width>
                <height>50</height>
                <visible>true</visible>
                <align>center</align>
                <aligny>center</aligny>
                <label>AUS</label>
                <font>Font_Reg29</font>
            </control>
        </control>
        <!--end switch buttons -->
    </controls>
</window>
