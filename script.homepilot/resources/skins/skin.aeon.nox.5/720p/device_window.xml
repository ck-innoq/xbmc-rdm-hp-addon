<window id="1234" type="dialog">
    <backgroundcolor>backgroundcolor</backgroundcolor>
    <allowoverlay>no</allowoverlay>
    <visible>true</visible>
    <coordinates>
        <system>1</system>
        <posX>200</posX>
        <posY>100</posY>
    </coordinates>
    <controls>
         <control type="image" id="1002">
            <posx>200</posx>
            <posy>20</posy>
            <width>470</width>
            <height>700</height>
            <texture border="40">dialog_back3.png</texture>
        </control>

        <!--title separator -->
        <control type="image" id="1004">
            <posx>230</posx>
            <posy>70</posy>
            <width>410</width>
            <height>30</height>
            <texture>separator2.png</texture>
        </control>
        <!-- end title separator -->


        <!--rollo buttons -->
        <control type="group" id="1141">
            <visible>true</visible>
            <posx>-350</posx>
            <posy>-420</posy>
            <height>100</height>
            <width>200</width>
            <control type="button" id="1131">
                <posx>70</posx>
                <posy>0</posy>
                <width>40</width>
                <height>25</height>
                <focusedcolor>white</focusedcolor>
		        <disabledcolor>grey4</disabledcolor>
                <texturenofocus>android/numberpicker_down_normal_holo_dark.png</texturenofocus>
		        <texturefocus>android/numberpicker_down_longpressed_holo_dark.png</texturefocus>
            </control>
            <control type="button" id="1121">
                <posx>125</posx>
                <posy>0</posy>
                <width>40</width>
                <height>25</height>
                <focusedcolor>white</focusedcolor>
		        <disabledcolor>grey4</disabledcolor>
                <texturenofocus>android/numberpicker_up_normal_holo_dark.png</texturenofocus>
		        <texturefocus>android/numberpicker_up_longpressed_holo_dark.png</texturefocus>
            </control>
        </control>
        <!--end up-down buttons -->

        <!--up-down buttons -->
        <control type="group" id="114">
            <visible>true</visible>
            <posx>-350</posx>
            <posy>-420</posy>
            <height>100</height>
            <width>200</width>
            <control type="button" id="112">
                <posx>70</posx>
                <posy>0</posy>
                <width>40</width>
                <height>25</height>
                <texturenofocus>android/numberpicker_down_normal_holo_dark.png</texturenofocus>
		        <texturefocus>android/numberpicker_down_longpressed_holo_dark.png</texturefocus>
            </control>
            <control type="button" id="113">
                <posx>125</posx>
                <posy>0</posy>
                <width>40</width>
                <height>25</height>
                <texturenofocus>android/numberpicker_up_normal_holo_dark.png</texturenofocus>
		        <texturefocus>android/numberpicker_up_longpressed_holo_dark.png</texturefocus>
            </control>
        </control>
        <!--end up-down buttons -->

        <!--switch buttons -->
        <control type="group" id="118">
            <visible>true</visible>
            <posx>-350</posx>
            <posy>-120</posy>
            <height>50</height>
            <width>150</width>
            <control type="label" id="115">
                <posx>110</posx>
                <posy>0</posy>
                <width>50</width>
                <height>50</height>
                <visible>true</visible>
                <align>center</align>
                <aligny>center</aligny>
                <label>AN</label>
                <font>Font_Reg29</font>
            </control>
            <control type="radiobutton" id="116">
                <posx>190</posx>
                <posy>0</posy>
                <width>30</width>
                <height>50</height>
                <visible>true</visible>
                <textureradioon>android/btn_radio_on_holo_dark.png</textureradioon>
                <textureradiooff>android/btn_radio_off_holo_dark.png</textureradiooff>
                <texturefocus border="5">-</texturefocus>
                <texturenofocus>-</texturenofocus>
                <focusedcolor>-</focusedcolor>
                <label>-</label>
                <align>center</align>
                <aligny>center</aligny>
            </control>
            <control type="label" id="117">
                <posx>200</posx>
                <posy>0</posy>
                <width>50</width>
                <height>50</height>
                <visible>true</visible>
                <align>center</align>
                <aligny>center</aligny>
                <label>AUS</label>
                <font>Font_Reg29</font>
            </control>
        </control>
        <!--end switch buttons -->

        <!--Automatik -->
        <control type="group" id="130">
            <visible>true</visible>
            <posx>320</posx>
            <posy>250</posy>
            <height>50</height>
            <width>150</width>
            <control type="radiobutton" id="132">
                <posx>0</posx>
                <posy>0</posy>
                <width>250</width>
                <height>50</height>
                <visible>true</visible>
                <textureradioon>android/btn_radio_on_holo_dark.png</textureradioon>
                <textureradiooff>android/btn_radio_off_holo_dark.png</textureradiooff>
                <texturefocus border="5">listselect_fo.png</texturefocus>
                <texturenofocus>-</texturenofocus>
                <focusedcolor>-</focusedcolor>
                <label>Automatik</label>
                <align>left</align>
                <aligny>center</aligny>
            </control>
        </control>
        <!--end Automatik -->


        <!--Favorit -->
        <control type="group" id="134">
            <visible>true</visible>
            <posx>320</posx>
            <posy>300</posy>
            <height>50</height>
            <width>150</width>
            <control type="radiobutton" id="136">
                <posx>0</posx>
                <posy>0</posy>
                <width>250</width>
                <height>50</height>
                <visible>true</visible>
                <textureradioon>favorite_20.png</textureradioon>
                <textureradiooff>favorite_off_20.png</textureradiooff>
                <texturefocus border="5">listselect_fo.png</texturefocus>
                <texturenofocus>-</texturenofocus>
                <focusedcolor>-</focusedcolor>
                <label>Favorit</label>
                <align>left</align>
                <aligny>center</aligny>
            </control>
        </control>
        <!--end Favorit -->

        <!--Automationen -->
        <control type="group" id="138">
            <visible>true</visible>
            <posx>260</posx>
            <posy>370</posy>
            <height>50</height>
            <width>340</width>
            <control type="label" id="140">
                <posx>50</posx>
                <posy>0</posy>
                <width>200</width>
                <height>50</height>
                <visible>true</visible>
                <aligny>center</aligny>
                <label>Automationen</label>
                <font>Font_Reg29</font>
            </control>
            <control type="list" id="142">
                <posx>0</posx>
                <posy>50</posy>
                <width>350</width>
                <height>270</height>
                <onright>144</onright>
                <pagecontrol>144</pagecontrol>
                <itemlayout height="90" width="350">
                    <control type="image">
                        <posx>5</posx>
                        <posy>5</posy>
                        <width>24</width>
                        <height>24</height>
                        <info>ListItem.Icon</info>
                    </control>
                    <control type="textbox">
                        <posx>40</posx>
                        <posy>0</posy>
                        <width>310</width>
                        <height>85</height>
                        <font>font13</font>
                        <wrapmultiline>true</wrapmultiline>
                        <textcolor>grey2</textcolor>
                        <selectedcolor>selected</selectedcolor>
                        <align>left</align>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                </itemlayout>
                <focusedlayout height="90" width="350">
                    <control type="image">
                        <posx>-10</posx>
                        <posy>0</posy>
                        <width>370</width>
                        <height>80</height>
                        <texture border="8">listselect_fo.png</texture>
                        <visible>Control.HasFocus(142)</visible>
                        <colordiffuse>$VAR[FocusTextureColorVar]</colordiffuse>
                    </control>
                    <control type="image">
                        <posx>5</posx>
                        <posy>5</posy>
                        <width>24</width>
                        <height>24</height>
                        <info>ListItem.Icon</info>
                    </control>
                    <control type="textbox">
                        <posx>40</posx>
                        <posy>0</posy>
                        <width>310</width>
                        <height>85</height>
                        <font>font13</font>
                        <wrapmultiline>true</wrapmultiline>
                        <textcolor>white</textcolor>
                        <selectedcolor>selected</selectedcolor>
                        <align>left</align>
                        <label>$INFO[ListItem.Label]</label>
                        <visible>Control.HasFocus(142)</visible>
                    </control>
                    <control type="textbox">
                        <posx>40</posx>
                        <posy>0</posy>
                        <width>310</width>
                        <height>85</height>
                        <font>font13</font>
                        <wrapmultiline>true</wrapmultiline>
                        <textcolor>grey2</textcolor>
                        <selectedcolor>selected</selectedcolor>
                        <align>left</align>
                        <label>$INFO[ListItem.Label]</label>
                        <visible>!Control.HasFocus(142)</visible>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="144">
                <description>scroll bar indicator for lists</description>
                <posx>360</posx>
                <posy>50</posy>
                <width>12</width>
                <height>250</height>
                <onleft>142</onleft>
                <onright>noop</onright>
                <orientation>vertical</orientation>
                <visible>true</visible>
                <texturesliderbackground>-</texturesliderbackground>
                <texturesliderbar colordiffuse="55ffffff" border="1">scrollbar/texturesliderbar2.png</texturesliderbar>
                <texturesliderbarfocus border="1">scrollbar/texturesliderbar2.png</texturesliderbarfocus>
            </control>
        </control>
        <!--end Automationen -->


    </controls>
</window>
